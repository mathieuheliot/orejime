{
  "name": "orejime",
  "version": "1.1.1",
  "description": "An easy to use consent manager that focuses on accessibility. A fork of Orejime.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathieuheliot/orejime.git"
  },
  "keywords": [
    "cookie",
    "consent",
    "gdpr",
    "compliance"
  ],
  "main": "lib/orejime.js",
  "module": "es/orejime.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "cjs": "cross-env BABEL_ENV=cjs babel --out-dir lib src",
    "es": "cross-env BABEL_ENV=es babel --out-dir es src",
    "umd": "cross-env BABEL_ENV=umd webpack",
    "copy-scss": "cp src/scss/orejime.scss dist/orejime.scss",
    "build": "npm run clean-build && npm run cjs && npm run es && cross-env NODE_ENV=production npm run umd && npm run copy-scss",
    "clean-build": "rm -Rf ./dist/orejime.{js,css,scss} && rm -Rf ./lib && rm -Rf ./es",
    "prepublishOnly": "npm run build"
  },
  "author": "Mathieu HÃ©liot <mathieu.heliot@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mathieuheliot/orejime/issues"
  },
  "homepage": "https://github.com/mathieuheliot/orejime#readme",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "react-modal": "^3.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-convert-to-json": "^0.1.0",
    "browser-sync": "^2.26.3",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "preact": "^8.2.9",
    "preact-compat": "^3.18.2",
    "sass-loader": "^6.0.7",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.7.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16"
  }
}
